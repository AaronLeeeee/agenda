{
  "name": "agenda",
  "version": "1.0.0",
  "description": "Light weight job scheduler for Node.js",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && npm run ava",
    "lint": "xo --verbose",
    "ava": "nyc ava --verbose --fail-fast"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agenda/agenda"
  },
  "keywords": [
    "job",
    "jobs",
    "cron",
    "delayed",
    "scheduler",
    "runner"
  ],
  "author": "Ryan Schmukler <ryan@slingingcode.com> (http://slingingcode.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agenda/agenda/issues"
  },
  "dependencies": {
    "cron": "~1.2.1",
    "date.js": "~0.3.1",
    "debug": "~3.0.0",
    "human-interval": "~0.1.3",
    "moment-timezone": "~0.5.0",
    "mongodb": "~2.2.10"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "coveralls": "^2.13.1",
    "delay": "^2.0.0",
    "nyc": "^11.1.0",
    "q": "^1.5.0",
    "uuid": "^3.1.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    },
    "envs": [
      "node"
    ]
  }
}
